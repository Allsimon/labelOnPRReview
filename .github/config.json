  "runners": [
    {
      "root": ".",
      "versioning": {
        "source": "milestones",
        "type": "SemVer"
      },
      "prereleaseName": "alpha",
      "sharedConfig": {
        "labels": {
          "Abandoned": {
            "requires": 1,
            "conditions": [
              {
                "type": "isAbandoned",
                "stale": 30
              }
            ]
          },
          "Stale": {
            "requires": 1,
