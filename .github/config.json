                "max": 1000
              }
            ]
          },
          "Size - XXL": {
            "requires": 1,
            "conditions": [
              {
                "type": "changesSize",
                "min": 1000
              }
            ]
          },
          "Accepted :heavy_check_mark:": {
            "requires": 1,
            "conditions": [
              {
                "type": "isApproved",
                "value": true,
                "required": 1
              }
            ]
          },
          "In Progress :gear:": {
            "requires": 1,
            "conditions": [
              {
                "type": "pendingReview",
                "value": false
              },
              {
                "type": "requestedChanges",
                "value": true
              },
              {
                "type": "isApproved",
                "value": false,
                "required": 1
              }
            ]
          },
          "Awaiting Review :eyes:": {
            "requires": 1,
            "conditions": [
              {
                "type": "pendingReview",
                "value": true
              }
            ]
          },
          "Changes Requested :diamonds:": {
            "requires": 1,
            "conditions": [
              {
                "type": "requestedChanges",
                "value": true
              }
            ]
          },
          "Type - Bugfix" : {
            "requires": 1,
            "conditions": [
              {
                "type": "branchMatches",
                "pattern": "/^bugfix\\//im"
              }
            ]
          },
          "Type - Feature" : {
            "requires": 1,
            "conditions": [
              {
                "type": "branchMatches",
                "pattern": "/^feature\\//im"
              }
            ]
          },
          "Type - Enhancement :sparkler:" : {
            "requires": 1,
            "conditions": [
              {
                "type": "branchMatches",
                "pattern": "/^Enhanc\\//im"
              },
              {
                "type": "branchMatches",
                "pattern": "/^Enhancement\\//im"
              }
            ]
          },
          "Type - Cleanup :broom:": {
            "requires": 1,
            "conditions": [
              {
                "type": "branchMatches",
                "pattern": "/^Cleanup\\//im"
              }
            ]
          },
          "Type - Documentation :black_nib:": {
            "requires": 1,
            "conditions": [
              {
                "type": "branchMatches",
                "pattern": "/^Doc\\//im"
              },
              {
                "type": "branchMatches",
                "pattern": "/^Documentation\\//im"
              }
            ]
          }
        }
      },
      "issue": {
        "ref": "develop",
        "labels": {
          "Enhancement Complete": {
            "requires": 5,
            "conditions": [
              {
                "type": "descriptionMatches",
                "pattern": "/### Process\\s*-\\s*Enhancement/im"
              },
              {
                "type": "descriptionMatches",
                "pattern": "/^- \\[x\\] has enhancement been accepted/im"
              },
              {
                "type": "descriptionMatches",
                "pattern": "/^- \\[x\\] solution has been proposed/im"
              },
              {
                "type": "descriptionMatches",
